{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Draw raffle winners at your Meetup event.",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "scripts": {
    "clean": "rimraf .cache .netlify ./public/build ./public/sw.js ./**/*.generated.css",
    "build": "run-s build:*",
    "build:css": "run-p \"generate:css -- --minify\"",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/sw.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "build:app": "remix build",
    "dev": "npm run build && run-p dev:*",
    "dev:css": "run-p \"generate:css -- --watch\"",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/sw.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "dev:app": "cross-env NODE_ENV=development netlify dev",
    "generate:css": "tailwindcss --input ./app/styles/app.css --output ./app/styles/app.generated.css",
    "format": "prettier --cache --write .",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=production netlify dev"
  },
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@faker-js/faker": "7.6.0",
    "@netlify/functions": "1.3.0",
    "@remix-run/netlify": "1.8.2",
    "@remix-run/node": "1.8.2",
    "@remix-run/react": "1.8.2",
    "@zag-js/number-input": "0.2.5",
    "@zag-js/react": "0.3.2",
    "clsx": "1.2.1",
    "meetup-randomizer": "3.1.0",
    "msw": "0.49.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-github-corner": "2.5.0",
    "spinners-react": "1.0.7",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.8.2",
    "@remix-run/eslint-config": "1.8.2",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@wkovacs64/prettier-config": "3.0.3",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "eslint": "8.29.0",
    "netlify-cli": "12.2.11",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.20",
    "postcss-import": "15.1.0",
    "prettier": "2.8.1",
    "prettier-plugin-tailwindcss": "0.2.1",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.5",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
}
