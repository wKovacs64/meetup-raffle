{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Draw raffle winners at your Meetup event.",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "scripts": {
    "clean": "rimraf .cache .netlify ./public/build ./public/sw.js",
    "build": "run-s build:*",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/sw.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "build:app": "remix build",
    "dev": "run-p dev:*",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/sw.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "dev:app": "cross-env NODE_ENV=development netlify dev",
    "format": "prettier --cache --write .",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=production netlify dev"
  },
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@faker-js/faker": "7.6.0",
    "@netlify/functions": "1.4.0",
    "@remix-run/netlify": "1.16.0",
    "@remix-run/node": "1.16.0",
    "@remix-run/react": "1.16.0",
    "@zag-js/number-input": "0.7.0",
    "@zag-js/react": "0.7.0",
    "clsx": "1.2.1",
    "meetup-randomizer": "3.1.0",
    "msw": "1.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-github-corner": "2.5.0",
    "spinners-react": "1.0.7",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "1.16.0",
    "@remix-run/eslint-config": "1.16.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@wkovacs64/prettier-config": "3.0.3",
    "autoprefixer": "10.4.14",
    "cross-env": "7.0.3",
    "eslint": "8.40.0",
    "netlify-cli": "15.0.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.23",
    "postcss-import": "15.1.0",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.2.8",
    "rimraf": "5.0.0",
    "semantic-release": "21.0.2",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=14"
  }
}
