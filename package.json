{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Draw raffle winners at your Meetup event.",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "scripts": {
    "clean": "rimraf .cache .netlify build ./public/sw.js",
    "build": "remix vite:build",
    "dev": "cross-env NODE_OPTIONS=\"--require tsx/cjs --require ./mocks/index.ts\" remix vite:dev",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "netlify serve",
    "typecheck": "tsc --build"
  },
  "dependencies": {
    "@faker-js/faker": "8.4.1",
    "@netlify/functions": "2.6.0",
    "@netlify/remix-adapter": "2.3.1",
    "@remix-pwa/sw": "3.0.3",
    "@remix-pwa/worker-runtime": "2.1.1",
    "@remix-run/node": "2.9.1",
    "@remix-run/react": "2.9.1",
    "@zag-js/number-input": "0.49.0",
    "@zag-js/react": "0.49.0",
    "isbot": "5.1.6",
    "meetup-randomizer": "3.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "spinners-react": "1.0.7",
    "zod": "3.23.6"
  },
  "devDependencies": {
    "@remix-pwa/dev": "3.0.5",
    "@remix-run/dev": "2.9.1",
    "@remix-run/serve": "2.9.1",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@wkovacs64/prettier-config": "3.0.3",
    "autoprefixer": "10.4.19",
    "cpy-cli": "5.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-plugin-wkovacs64": "16.2.0",
    "glob": "10.3.12",
    "msw": "2.2.14",
    "netlify-cli": "17.23.1",
    "npm-run-all2": "6.1.2",
    "postcss": "8.4.38",
    "postcss-import": "16.1.0",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "rimraf": "5.0.5",
    "semantic-release": "23.0.8",
    "tailwindcss": "3.4.3",
    "tsx": "4.9.0",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-plugin-static-copy": "1.0.4",
    "vite-tsconfig-paths": "4.3.2"
  },
  "overrides": {
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=20"
  }
}
