  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "scripts": {
    "build": "vite build",
    "clean": "rimraf dist coverage",
    "cm": "git-cz",
    "dev": "netlify dev",
    "format": "prettier --write .",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "start": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "validate": "npm-run-all --silent --parallel lint build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@commitlint/cli": "17.0.1",
    "@commitlint/config-conventional": "17.0.0",
    "@faker-js/faker": "7.1.0",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.0",
    "@types/node": "16.11.36",
    "@vitejs/plugin-react": "1.3.2",
    "@wkovacs64/prettier-config": "3.0.2",
    "@xstate/inspect": "0.7.0",
    "@xstate/react": "3.0.0",
    "autoprefixer": "10.4.7",
    "c8": "7.11.3",
    "clsx": "1.1.1",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.16.0",
    "eslint-plugin-wkovacs64": "13.13.0",
    "husky": "8.0.1",
    "is-number": "7.0.0",
    "jsdom": "19.0.0",
    "lint-staged": "12.4.3",
    "meetup-randomizer": "3.1.0",
    "msw": "0.42.0",
    "netlify-cli": "10.4.0",
    "node-fetch": "2.6.7",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.11",
    "prop-types": "15.8.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-github-corner": "2.5.0",
    "react-test-renderer": "18.1.0",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2",
    "spinners-react": "1.0.7",
    "tailwindcss": "3.0.24",
    "typescript": "4.7.2",
    "unfetch": "4.2.0",
    "use-stepper": "3.0.0",
    "vite": "2.9.9",
    "vite-plugin-pwa": "0.12.0",
    "vitest": "0.13.0",
    "workbox-build": "6.5.3",
    "workbox-window": "6.5.3",
    "ws": "8.7.0",
    "xstate": "4.32.1"
  }
