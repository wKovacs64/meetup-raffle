{
  "name": "meetup-raffle",
  "version": "3.0.3-dev.0",
  "description": "Select a raffle winner at your upcoming Meetup.",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "scripts": {
    "build": "yarn build:client && yarn build:functions",
    "build:client": "react-scripts build",
    "build:functions": "netlify-lambda build src/functions",
    "clean": "rimraf build functions coverage",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "start": "cross-env NODE_ENV=development REACT_APP_DRAW_URL=http://localhost:9000/draw concurrently --names \"CLIENT,FUNCTIONS\" --prefix-colors \"bgRed.bold,bgBlue.bold\" \"yarn start:client\" \"yarn run start:functions\"",
    "start:client": "react-scripts start",
    "start:functions": "netlify-lambda serve src/functions",
    "test": "cross-env REACT_APP_DRAW_URL=http://localhost:9000/draw react-scripts test --env=jsdom"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/client/**/*.js",
      "src/functions/**/*.js"
    ]
  },
  "private": true,
  "dependencies": {
    "axios": "0.18.0",
    "core-js": "2.5.7",
    "emotion": "9.2.4",
    "formik": "0.11.11",
    "is-number": "6.0.0",
    "lodash.get": "4.4.2",
    "meetup-randomizer": "3.1.0",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-css-collapse": "3.1.0",
    "react-dom": "16.4.1",
    "react-github-corner": "2.3.0",
    "react-scripts": "1.1.4",
    "react-spinners": "0.3.2",
    "react-stepper-primitive": "1.1.0",
    "tachyons": "4.10.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.5",
    "concurrently": "3.6.0",
    "coveralls": "3.0.1",
    "cross-env": "5.2.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.0.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.10.0",
    "husky": "0.14.3",
    "jest-emotion": "9.2.4",
    "lint-staged": "7.2.0",
    "netlify-lambda": "0.4.0",
    "prettier": "1.13.6",
    "react-testing-library": "4.0.2",
    "rimraf": "2.6.2"
  }
}
