{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Select a raffle winner at your upcoming Meetup.",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "scripts": {
    "build": "npm-run-all --silent --parallel build:*",
    "build:client": "react-scripts build",
    "build:functions": "netlify-lambda build src/functions",
    "clean": "rimraf build functions coverage",
    "cm": "git-cz",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{html,js,json,md,yml,yaml}\"",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=development concurrently --names \"CLIENT,FUNCTIONS\" --prefix-colors \"bgRed.bold,bgBlue.bold\" \"yarn start:client\" \"yarn run start:functions\"",
    "start:client": "react-scripts start",
    "start:functions": "netlify-lambda serve src/functions",
    "test": "react-scripts test",
    "validate": "npm-run-all --silent --parallel lint build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/client/**/*.js",
      "src/functions/**/*.js"
    ],
    "snapshotSerializers": [
      "jest-emotion"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.14",
    "axios": "0.19.0",
    "core-js-pure": "3.1.4",
    "formik": "1.5.7",
    "is-number": "7.0.0",
    "lodash": "4.17.11",
    "meetup-randomizer": "3.1.0",
    "prop-types": "15.7.2",
    "react": "16.9.0-alpha.0",
    "react-dom": "16.9.0-alpha.0",
    "react-github-corner": "2.3.0",
    "react-scripts": "3.0.1",
    "react-spinners": "0.5.4",
    "tachyons": "4.11.1",
    "use-stepper": "2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@testing-library/react": "8.0.4",
    "@wkovacs64/eslint-config-react": "2.2.3",
    "codecov": "3.5.0",
    "commitizen": "3.1.1",
    "concurrently": "4.1.1",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint-plugin-emotion": "10.0.14",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "22.7.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "http-proxy-middleware": "0.19.1",
    "husky": "3.0.0",
    "jest-dom": "3.5.0",
    "jest-emotion": "10.0.14",
    "lint-staged": "9.0.2",
    "netlify-lambda": "1.4.13",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "react-hooks-testing-library": "0.6.0",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.18"
  }
}
