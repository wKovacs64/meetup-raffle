{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Select a raffle winner at your upcoming Meetup.",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "scripts": {
    "build": "npm-run-all --silent --parallel build:*",
    "build:client": "react-scripts build",
    "build:functions": "netlify-lambda build src/functions",
    "clean": "rimraf build functions coverage",
    "cm": "git-cz",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{html,js,json,md,yml,yaml}\"",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "start": "cross-env NODE_ENV=development concurrently --names \"CLIENT,FUNCTIONS\" --prefix-colors \"bgRed.bold,bgBlue.bold\" \"yarn start:client\" \"yarn run start:functions\"",
    "start:client": "react-scripts start",
    "start:functions": "netlify-lambda serve src/functions",
    "test": "react-scripts test",
    "validate": "npm-run-all --silent --parallel lint build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/client/**/*.js",
      "src/functions/**/*.js"
    ],
    "snapshotSerializers": [
      "jest-emotion"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "core-js-pure": "3.6.4",
    "formik": "2.1.4",
    "is-number": "7.0.0",
    "lodash": "4.17.15",
    "meetup-randomizer": "3.1.0",
    "node-fetch": "2.6.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-github-corner": "2.3.0",
    "react-scripts": "3.4.1",
    "react-spinners": "0.8.1",
    "tachyons": "4.11.1",
    "unfetch": "4.1.0",
    "use-stepper": "2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@sheerun/mutationobserver-shim": "0.3.3",
    "@testing-library/jest-dom": "5.4.0",
    "@testing-library/react": "10.0.2",
    "@wkovacs64/prettier-config": "2.0.0",
    "codecov": "3.6.5",
    "commitizen": "4.0.4",
    "concurrently": "5.1.0",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.1.0",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-wkovacs64": "3.0.0",
    "fetch-mock-jest": "1.2.5",
    "http-proxy-middleware": "1.0.3",
    "husky": "4.2.4",
    "jest-emotion": "10.0.32",
    "lint-staged": "10.1.3",
    "netlify-lambda": "1.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.4",
    "react-hooks-testing-library": "0.6.0",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.0.4"
  }
}
