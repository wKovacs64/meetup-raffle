{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Draw raffle winners at your Meetup event.",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "scripts": {
    "clean": "rimraf .cache .netlify ./public/build ./public/sw.js",
    "build": "run-s build:*",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/sw.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "build:app": "remix build",
    "dev": "run-p dev:*",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/sw.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "dev:app": "remix dev --no-restart --command \"binode --require @esbuild-kit/cjs-loader --require ./mocks/index.ts -- @remix-run/serve:remix-serve .netlify/functions-internal/server.js\"",
    "format": "prettier --cache --write .",
    "lint": "eslint .",
    "start": "netlify serve",
    "typecheck": "tsc --noEmit"
  },
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@faker-js/faker": "8.0.2",
    "@netlify/functions": "1.6.0",
    "@remix-run/netlify": "1.18.1",
    "@remix-run/node": "1.18.1",
    "@remix-run/react": "1.18.1",
    "@zag-js/number-input": "0.11.2",
    "@zag-js/react": "0.11.2",
    "meetup-randomizer": "3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-github-corner": "2.5.0",
    "spinners-react": "1.0.7",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "2.4.2",
    "@remix-run/dev": "1.18.1",
    "@remix-run/eslint-config": "1.18.1",
    "@remix-run/serve": "1.18.1",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@wkovacs64/prettier-config": "3.0.3",
    "autoprefixer": "10.4.14",
    "binode": "1.0.5",
    "eslint": "8.44.0",
    "msw": "0.0.0-fetch.rc-15",
    "netlify-cli": "15.9.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.26",
    "postcss-import": "15.1.0",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.4.0",
    "rimraf": "5.0.1",
    "semantic-release": "21.0.7",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=14"
  }
}
