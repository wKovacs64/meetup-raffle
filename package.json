    "react": "^16.2.0",
    "react-css-collapse": "^3.1.0",
    "react-dom": "^16.2.0",
    "react-github-corner": "^2.1.0",
    "react-scripts": "1.1.1",
    "react-stepper-primitive": "^1.1.0",
    "serve": "^6.5.0",
    "tachyons": "^4.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.1",
