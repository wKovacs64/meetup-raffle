{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Draw raffle winners at your Meetup event.",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "scripts": {
    "build": "vite build",
    "clean": "rimraf dist coverage",
    "cm": "git-cz",
    "dev": "netlify dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "start": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --onlyChanged",
    "validate": "npm-run-all --silent --parallel lint build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@babel/core": "7.17.5",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@emotion/jest": "11.8.0",
    "@emotion/react": "11.8.2",
    "@emotion/styled": "11.8.1",
    "@faker-js/faker": "6.0.0-beta.0",
    "@mdx-js/react": "1.6.22",
    "@rushstack/eslint-patch": "1.1.0",
    "@testing-library/dom": "8.11.3",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0",
    "@types/node": "16.11.26",
    "@vitejs/plugin-react": "1.2.0",
    "@wkovacs64/normalize.css": "8.0.4",
    "@wkovacs64/prettier-config": "3.0.0",
    "@xstate/inspect": "0.6.4",
    "@xstate/react": "2.0.1",
    "babel-jest": "27.5.1",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.10.0",
    "eslint-plugin-wkovacs64": "13.12.0",
    "husky": "7.0.4",
    "is-number": "7.0.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.5",
    "meetup-randomizer": "3.1.0",
    "msw": "0.39.1",
    "netlify-cli": "9.13.0",
    "node-fetch": "2.6.7",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-github-corner": "2.5.0",
    "react-spinners": "0.11.0",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2",
    "theme-ui": "0.13.1",
    "typescript": "4.6.2",
    "unfetch": "4.2.0",
    "use-stepper": "2.0.3",
    "vite": "2.8.6",
    "vite-plugin-pwa": "0.11.13",
    "workbox-build": "6.5.1",
