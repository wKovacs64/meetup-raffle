    "eslint-plugin-wkovacs64": "13.8.0",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "lint-staged": "12.1.4",
    "netlify-cli": "8.4.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
