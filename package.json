{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Draw raffle winners at your Meetup event.",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "prettier": "@wkovacs64/prettier-config",
  "msw": {
    "workerDirectory": "public"
  },
  "scripts": {
    "clean": "rimraf .cache .netlify build ./public/sw.js",
    "build": "remix vite:build",
    "dev": "cross-env NODE_OPTIONS=\"--require tsx/cjs --require ./mocks/index.ts\" remix vite:dev",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "netlify serve",
    "typecheck": "tsc --build"
  },
  "dependencies": {
    "@faker-js/faker": "8.4.1",
    "@netlify/functions": "2.8.1",
    "@netlify/remix-adapter": "2.4.0",
    "@remix-pwa/sw": "3.0.9",
    "@remix-pwa/worker-runtime": "2.1.4",
    "@remix-run/node": "2.11.2",
    "@remix-run/react": "2.11.2",
    "@zag-js/number-input": "0.65.1",
    "@zag-js/react": "0.65.1",
    "isbot": "5.1.17",
    "meetup-randomizer": "3.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "spinners-react": "1.0.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@remix-pwa/dev": "3.1.0",
    "@remix-run/dev": "2.11.2",
    "@remix-run/serve": "2.11.2",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@wkovacs64/eslint-config": "7.2.5",
    "@wkovacs64/prettier-config": "4.1.1",
    "autoprefixer": "10.4.20",
    "cpy-cli": "5.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "glob": "11.0.0",
    "msw": "2.4.1",
    "netlify-cli": "17.34.3",
    "npm-run-all2": "6.2.2",
    "postcss": "8.4.42",
    "postcss-import": "16.1.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.6",
    "rimraf": "6.0.1",
    "semantic-release": "24.1.0",
    "tailwindcss": "3.4.10",
    "tsx": "4.19.0",
    "typescript": "5.5.4",
    "vite": "5.3.5",
    "vite-plugin-static-copy": "1.0.6",
    "vite-tsconfig-paths": "5.0.1"
  },
  "overrides": {
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=20"
  }
}
