{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Draw raffle winners at your Meetup event.",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "scripts": {
    "build": "npm-run-all --silent --parallel build:*",
    "build:client": "vite build",
    "build:functions": "netlify-lambda build src/functions",
    "clean": "rimraf dist functions coverage",
    "cm": "git-cz",
    "dev": "netlify dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "start": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --onlyChanged",
    "validate": "npm-run-all --silent --parallel lint build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@wkovacs64/normalize.css": "8.0.3",
    "@xstate/inspect": "0.4.1",
    "@xstate/react": "1.5.1",
    "faker": "5.5.3",
    "is-number": "7.0.0",
    "meetup-randomizer": "3.1.0",
    "msw": "0.30.1",
    "node-fetch": "2.6.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-github-corner": "2.5.0",
    "react-spinners": "0.11.0",
    "theme-ui": "0.10.0",
    "use-stepper": "2.0.3",
    "xstate": "4.22.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@emotion/jest": "11.3.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.1.9",
    "@vitejs/plugin-react-refresh": "1.3.5",
    "@wkovacs64/prettier-config": "3.0.0",
    "babel-jest": "27.0.6",
    "codecov": "3.8.2",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.30.0",
    "eslint-plugin-wkovacs64": "13.3.1",
    "husky": "6.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.0.0",
    "netlify-cli": "4.1.13",
    "netlify-lambda": "2.0.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.4",
    "unfetch": "4.2.0",
    "vite": "2.4.1",
    "vite-plugin-pwa": "0.8.1",
    "workbox-background-sync": "6.1.5",
    "workbox-broadcast-update": "6.1.5",
    "workbox-cacheable-response": "6.1.5",
    "workbox-core": "6.1.5",
    "workbox-expiration": "6.1.5",
    "workbox-google-analytics": "6.1.5",
    "workbox-navigation-preload": "6.1.5",
    "workbox-precaching": "6.1.5",
    "workbox-range-requests": "6.1.5",
    "workbox-routing": "6.1.5",
