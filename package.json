{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Draw raffle winners at your Meetup event.",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "scripts": {
    "build": "npm-run-all --silent --parallel build:*",
    "build:client": "react-scripts build",
    "build:functions": "netlify-lambda build src/functions",
    "clean": "rimraf build functions coverage",
    "cm": "git-cz",
    "develop": "netlify dev",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "postinstall": "husky install",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "validate": "npm-run-all --silent --parallel lint build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/client/**/*.js",
      "src/functions/**/*.js",
      "!src/client/dev-tools/**/*.js",
      "!**/index.js"
    ],
    "resetMocks": false,
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ]
  },
  "dependencies": {
    "@wkovacs64/normalize.css": "8.0.1",
    "@xstate/inspect": "0.4.1",
    "@xstate/react": "1.3.1",
    "faker": "5.4.0",
    "is-number": "7.0.0",
    "meetup-randomizer": "3.1.0",
    "msw": "0.27.1",
    "node-fetch": "2.6.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-github-corner": "2.5.0",
    "react-scripts": "4.0.3",
    "react-spinners": "0.10.6",
    "theme-ui": "0.5.0-alpha.2",
    "unfetch": "4.2.0",
    "use-stepper": "2.0.3",
    "web-vitals": "1.1.0",
    "workbox-background-sync": "5.1.4",
    "workbox-broadcast-update": "5.1.4",
    "workbox-cacheable-response": "5.1.4",
    "workbox-core": "5.1.4",
    "workbox-expiration": "5.1.4",
    "workbox-google-analytics": "5.1.4",
    "workbox-navigation-preload": "5.1.4",
    "workbox-precaching": "5.1.4",
    "workbox-range-requests": "5.1.4",
    "workbox-routing": "5.1.4",
    "workbox-strategies": "5.1.4",
    "workbox-streams": "5.1.4",
    "xstate": "4.16.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@emotion/jest": "11.2.1",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.8.3",
    "@wkovacs64/prettier-config": "3.0.0",
    "codecov": "3.8.1",
    "commitizen": "4.2.3",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.21.0",
    "eslint-plugin-wkovacs64": "12.0.0",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "netlify-cli": "3.10.10",
    "netlify-lambda": "2.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "react-hooks-testing-library": "0.6.0",
    "react-test-renderer": "17.0.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.1"
  }
}
