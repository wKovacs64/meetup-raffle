{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Draw raffle winners at your Meetup event.",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "scripts": {
    "clean": "rimraf .cache .netlify ./public/build ./public/sw.js",
    "build": "run-s build:*",
    "build:worker": "remix-pwa build",
    "build:app": "remix build",
    "dev": "run-p dev:*",
    "dev:worker": "remix-pwa dev",
    "dev:app": "remix dev --manual --command \"binode --require @esbuild-kit/cjs-loader --require ./mocks/index.ts -- @remix-run/serve:remix-serve .netlify/functions-internal/server.js\"",
    "format": "prettier --cache --write .",
    "lint": "eslint .",
    "start": "netlify serve",
    "typecheck": "tsc --noEmit"
  },
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@faker-js/faker": "8.3.1",
    "@netlify/functions": "1.6.0",
    "@netlify/remix-adapter": "1.0.0",
    "@remix-pwa/cache": "2.0.12",
    "@remix-pwa/strategy": "2.1.9",
    "@remix-pwa/sw": "2.1.12",
    "@remix-run/node": "2.3.0",
    "@remix-run/react": "2.3.0",
    "@zag-js/number-input": "0.29.0",
    "@zag-js/react": "0.29.0",
    "meetup-randomizer": "3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-github-corner": "2.5.0",
    "source-map-support": "0.5.21",
    "spinners-react": "1.0.7",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "2.4.4",
    "@remix-pwa/dev": "2.0.31",
    "@remix-pwa/worker-runtime": "2.0.8",
    "@remix-run/dev": "2.3.0",
    "@remix-run/eslint-config": "2.3.0",
    "@remix-run/serve": "2.3.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/source-map-support": "0.5.10",
    "@wkovacs64/prettier-config": "3.0.3",
    "autoprefixer": "10.4.16",
    "binode": "1.0.5",
    "eslint": "8.53.0",
    "glob": "10.3.10",
    "msw": "2.0.7",
    "netlify-cli": "17.4.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "postcss-import": "15.1.0",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "remix-pwa": "3.0.19",
    "rimraf": "5.0.5",
    "semantic-release": "22.0.7",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
