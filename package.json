{
  "name": "meetup-raffle",
  "version": "0.0.0-semantically-released",
  "description": "Draw raffle winners at your Meetup event.",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/meetup-raffle.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/meetup-raffle/issues"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "scripts": {
    "clean": "rimraf .cache .netlify build ./public/build ./public/_redirects ./public/sw.js",
    "build": "run-s build:*",
    "build:worker": "remix-pwa build",
    "build:app": "npm run redirects:enable && remix build",
    "dev": "run-p dev:*",
    "dev:worker": "remix-pwa dev",
    "dev:app": "npm run redirects:disable && cross-env NODE_OPTIONS=\"--require tsx/cjs --require ./mocks/index.ts\" remix dev --manual",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "eslint .",
    "redirects:enable": "cpy .redirects --rename _redirects public",
    "redirects:disable": "rimraf public/_redirects",
    "start": "netlify serve",
    "typecheck": "tsc --build"
  },
  "dependencies": {
    "@faker-js/faker": "8.3.1",
    "@netlify/functions": "2.5.1",
    "@netlify/remix-adapter": "2.2.0",
    "@remix-pwa/cache": "2.0.12",
    "@remix-pwa/strategy": "2.1.9",
    "@remix-pwa/sw": "2.1.12",
    "@remix-run/node": "2.5.1",
    "@remix-run/react": "2.5.1",
    "@zag-js/number-input": "0.32.1",
    "@zag-js/react": "0.32.1",
    "meetup-randomizer": "3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "spinners-react": "1.0.7",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@remix-pwa/dev": "2.0.31",
    "@remix-pwa/worker-runtime": "2.0.8",
    "@remix-run/dev": "2.5.1",
    "@remix-run/eslint-config": "2.5.1",
    "@remix-run/serve": "2.5.1",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@wkovacs64/prettier-config": "3.0.3",
    "autoprefixer": "10.4.17",
    "cpy-cli": "5.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "glob": "10.3.10",
    "msw": "2.1.3",
    "netlify-cli": "17.15.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.33",
    "postcss-import": "16.0.0",
    "prettier": "3.2.4",
    "prettier-plugin-tailwindcss": "0.5.11",
    "remix-pwa": "3.0.19",
    "rimraf": "5.0.5",
    "semantic-release": "23.0.0",
    "tailwindcss": "3.4.1",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=20"
  }
}
